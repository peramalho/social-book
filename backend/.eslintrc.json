{
    "env": {
        "browser": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": [
      "prettier",
      "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "import/extensions": 0,
      "no-console": 0
    }
}
