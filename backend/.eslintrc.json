{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": [
      "airbnb-base", 
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "no-underscore-dangle": "off" 
    }
}
